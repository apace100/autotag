{
  "schemaVersion": 1,
  "id": "autotag-convention",
  "version": "${version}",

  "name": "AutoTag Convention",
  "description": "Automatically fills some convention tags using AutoTag API.",
  "authors": [
    "Apace",
    "Will BL"
  ],

  "license": "MIT",
  "icon": "assets/autotag/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.apace100.autotag.common.AutoTagConvention"
    ]
  },

  "depends": {
    "fabricloader": ">=0.13.3",
    "minecraft": ">=1.20",
    "java": ">=17",
    "autotag": "*"
  },

  "custom": {
    "mc-publish": {
      "modrinth": "8FdYDHF5",
      "curseforge": "618433",
      "loaders": ["fabric"],
      "dependencies": [
        "autotag(embedded)#(ignore:modrinth,curseforge,github)"
      ]
    }
  }
}
